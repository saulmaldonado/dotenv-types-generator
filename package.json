{
  "name": "dotenv-types-generator",
  "version": "1.0.1",
  "description": "Generates type declarations for your .env file",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/saulmaldonado/dotenv-types-generator.git"
  },
  "keywords": [
    "typescript",
    ".env",
    "dotenv",
    "cli",
    "types"
  ],
  "author": "Saul Maldonado <samaldonado4@gmail.com>",
  "bin": "./build/index.js",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --fix",
    "prettier": "prettier --write",
    "format": "yarn lint && yarn prettier",
    "test": "jest",
    "start": "yarn build && dotenv-types-generator",
    "build": "rollup -c && chmod +x build/index.js"
  },
  "dependencies": {
    "yargs": "^16.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@rollup/plugin-babel": "^5.2.1",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.2",
    "husky": ">=4",
    "jest": "^26.4.2",
    "lint-staged": ">=10",
    "prettier": "^2.1.2",
    "rollup": "^2.28.2",
    "rollup-plugin-preserve-shebang": "^1.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
